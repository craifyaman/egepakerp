@model StokHareket
<!--begin::Form-->
<form id="StokHareketForm">
    @Html.HiddenFor(x => x.StokHareketId)
    <div class="form-group row">
        <div class="col-12">
            <label>Adı:</label>
            @Html.TextBoxFor(x => x.Adi, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group row">
      
        <div class="col-12">
            <label>Adet:</label>
            @Html.TextBoxFor(x => x.Adet, new { @class = "form-control" })
        </div>

        <div class="col-12">
            <label>Yer:</label>
            @Html.TextBoxFor(x => x.Yer, new { @class = "form-control" })
        </div>

    </div>

</form>
<!--end::Form-->
